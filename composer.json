{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.2",
        "aws/aws-sdk-php": "^3.314",
        "barryvdh/laravel-dompdf": "*",
        "doctrine/dbal": "^3.0",
        "guzzlehttp/guzzle": "^7.9",
        "hfig/mapi": "^1.4",
        "kyslik/column-sortable": "^7.0",
        "laravel/framework": "^12.0",
        "laravel/helpers": "*",
        "laravel/passport": "^12.0",
        "laravel/socialite": "^5.11",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.3",
        "league/flysystem-aws-s3-v3": "^3.0",
        "maatwebsite/excel": "^3.1.48",
        "mews/captcha": "^3.3",
        "mikehaertl/php-pdftk": "0.13.1",
        "php-mime-mail-parser/php-mime-mail-parser": "^1.0",
        "phpoffice/phpspreadsheet": "^1.29",
        "phpoffice/phpword": "^1.3",
        "salmanzafar/laravel-geocode": "^1.0",
        "setasign/fpdf": "^1.8",
        "setasign/fpdi": "^2.6",
        "smalot/pdfparser": "^2.12",
        "spatie/laravel-activitylog": "^4.7",
        "spatie/laravel-html": "^3.12",
        "spatie/laravel-ignition": "^2.0",
        "spatie/laravel-permission": "^6.18",
        "spatie/laravel-query-builder": "^6.0",
        "spatie/pdf-to-image": "^3.1",
        "stripe/stripe-php": "^13.13",
        "symfony/http-client": "^7.0",
        "symfony/mailgun-mailer": "^7.0",
        "symfony/postmark-mailer": "^7.0",
        "tecnickcom/tcpdf": "^6.10",
        "twilio/sdk": "^7.13",
        "yajra/laravel-datatables-oracle": "^12.0"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "mockery/mockery": "^1.4.4",
        "phpunit/phpunit": "^10.1",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.43",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "kylekatarnls/update-helper": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
